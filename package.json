{
  "name": "opengoat",
  "version": "0.1.0",
  "description": "OpenGoat agent orchestrator CLI",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "opengoat": "bin/opengoat"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist coverage",
    "dev": "tsx src/apps/cli/index.ts",
    "start": "node bin/opengoat",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "version-packages": "node scripts/release.mjs",
    "release": "node scripts/release.mjs && npm publish"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^22.13.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.14.1",
    "@clack/prompts": "^1.0.0"
  }
}
