{
  "name": "@opengoat/opengoat-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "OpenGoat monorepo workspace",
  "type": "module",
  "packageManager": "pnpm@10.18.2",
  "scripts": {
    "build": "pnpm --filter @opengoat/core build && pnpm --filter @opengoat/ui build && pnpm --filter opengoat build",
    "clean": "pnpm --filter @opengoat/core clean && pnpm --filter @opengoat/ui clean && pnpm --filter opengoat clean && rm -rf coverage",
    "dev": "pnpm --filter opengoat dev",
    "start": "pnpm --filter opengoat start",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "pnpm --filter @opengoat/core typecheck && pnpm --filter opengoat typecheck",
    "changeset": "changeset",
    "version-packages": "node scripts/release.mjs",
    "release": "node scripts/release.mjs"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "pnpm": {
    "overrides": {
      "lodash-es": ">=4.17.23",
      "@trpc/client": "10.45.4",
      "@trpc/server": "10.45.4"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^22.13.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.14.1",
    "sql.js": "^1.13.0"
  }
}
